{
    "name": "transformPug",
    "description": "directus extension to convert pug to html and js"
    "icon": "extension",
    "version": "1.0.0",
    "keywords": [
	"directus",
	"directus-extension",
	"directus-extension-operation"
    ],
    "type": "module",
    "files": [
	"dist"
    ],
    "directus:extension": {
	"type": "operation",
	"path": {
	    "app": "dist/app.js",
	    "api": "dist/api.js"
	},
	"source": {
	    "app": "src/app.ts",
	    "api": "src/api.ts"
	},
	"host": "^10.10.0"
    },
    "scripts": {
	"build": "directus-extension build",
	"dev": "directus-extension build -w --no-minify",
	"link": "directus-extension link"
    },
    "devDependencies": {
	"@directus/extensions-sdk": "12.0.1",
	"@types/node": "^22.5.0",
	"typescript": "^5.5.4",
	"vue": "^3.4.38"
    },
    "dependencies": {
	"marked": "^14.1.1",
	"pug": "^3.0.3"
    }
}
